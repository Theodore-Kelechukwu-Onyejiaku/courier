<%- include("../partials/admin/header") %>
<h3 style="color:black">Deposits</h3>

<h3 style="color:green"><% var message %> <%= message %></h3>
<table>
    <thead>
        <tr>
            <th>Username</th>
            
            <th>View</th>
            <th>No of deposits</th>
        </tr>
    </thead>
    <tbody>
        <tbody>
            <% user.forEach(each =>{%>
                <tr>
                    <td><%= each.username %></td>
                    <td><a href="/admin/deposits/<%= each._id %>" class="btn btn-primary"> <i class="fa fa-eye"></i></a></td>
                    <td><%= each.deposit.length %></td>
                </tr>
            <%}) %>
        </tbody>
    </tbody>
</table>


<%- include("../partials/admin/footer") %> -->